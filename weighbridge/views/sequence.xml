<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <record id="weighbridge_log" model="ir.sequence">
            <field name="name">WeighBridge</field>
            <field name="code">weighbridge.log.sequence</field>
            <field name="prefix">WB/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="quality_log" model="ir.sequence">
            <field name="name">Quality</field>
            <field name="code">quality.log.sequence</field>
            <field name="prefix">QC/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
